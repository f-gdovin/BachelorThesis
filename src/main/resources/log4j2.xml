<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG">
    <Appenders>
        <Console name="terminal">
            <param name="Threshold" value="DEBUG"/>
            <param name="Target" value="System.out"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="cz.muni.fgdovin.bachelorthesis" level="debug">
            <AppenderRef ref="terminal"/>
        </Logger>
        <Root level="DEBUG">
            <appender-ref ref="terminal"/>
        </Root>
    </Loggers>
</Configuration>