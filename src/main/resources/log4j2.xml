<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
    <Appenders>
        <Console name="terminal">
            <param name="Threshold" value="INFO"/>
            <param name="Target" value="System.out"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="cz.muni.fgdovin.bachelorthesis" level="info">
            <AppenderRef ref="terminal"/>
        </Logger>
        <Root level="info">
            <appender-ref ref="terminal"/>
        </Root>
    </Loggers>
</Configuration>